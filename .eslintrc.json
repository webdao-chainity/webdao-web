{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "google",
    "plugin:@next/next/recommended",
    "eslint:recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "@typescript-eslint",
    "react-hooks"
  ],
  "rules": {
    "require-jsdoc": 0,
    "react/react-in-jsx-scope": 0,
    "no-unused-vars": 0,
    "valid-jsdoc": 0,
    "spaced-comment": 0,
    "no-undef": 0,
    "react-hooks/rules-of-hooks": "error"
  }
}
